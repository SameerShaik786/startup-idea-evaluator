{
  "$defs": {
    "ReportSection": {
      "properties": {
        "title": {
          "title": "Title",
          "type": "string"
        },
        "content": {
          "title": "Content",
          "type": "string"
        },
        "score": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "title": "Score"
        }
      },
      "required": [
        "title",
        "content",
        "score"
      ],
      "title": "ReportSection",
      "type": "object"
    }
  },
  "description": "Final consolidated report.",
  "properties": {
    "schema_version": {
      "default": "1.0.0",
      "description": "Schema version",
      "title": "Schema Version",
      "type": "string"
    },
    "created_at": {
      "description": "Creation timestamp",
      "format": "date-time",
      "title": "Created At",
      "type": "string"
    },
    "agent_name": {
      "description": "Name of the agent that generated this output",
      "title": "Agent Name",
      "type": "string"
    },
    "agent_version": {
      "description": "Version of the agent model used",
      "title": "Agent Version",
      "type": "string"
    },
    "model_name": {
      "description": "LLM model used (e.g., gpt-4, claude-3)",
      "title": "Model Name",
      "type": "string"
    },
    "startup_id": {
      "format": "uuid",
      "title": "Startup Id",
      "type": "string"
    },
    "overall_score": {
      "description": "Final evaluation score",
      "maximum": 100,
      "minimum": 0,
      "title": "Overall Score",
      "type": "integer"
    },
    "sections": {
      "description": "Detailed report sections",
      "items": {
        "$ref": "#/$defs/ReportSection"
      },
      "title": "Sections",
      "type": "array"
    },
    "recommendation": {
      "description": "Final recommendation (e.g., Invest, Watch, Pass)",
      "title": "Recommendation",
      "type": "string"
    },
    "generated_at": {
      "format": "date-time",
      "title": "Generated At",
      "type": "string"
    }
  },
  "required": [
    "agent_name",
    "agent_version",
    "model_name",
    "startup_id",
    "overall_score",
    "recommendation"
  ],
  "title": "ReportOutput",
  "type": "object"
}